{
	"$schema": "https://mintlify.com/schema.json",
	"name": "DinoHub",
	"logo": {
		"dark": "/logo/logo.svg",
		"light": "/logo/u-light.svg"
	},
	"favicon": "/logo/logo.svg",
	"colors": {
		"primary": "#25C2A0",
		"light": "#25C2A0",
		"dark": "#25C2A0",
		"background": {
			"dark": "#0c0c0c",
			"light": "#fcfcfc"
		},
		"anchors": {
			"from": "#25C2A0",
			"to": "#16a889"
		}
	},
	"modeToggle": {
		"default": "dark",
		"isHidden": true
	},
	"background": {
		"style": "grid"
	},
	"anchors": [
		{
			"name": "Discord Community",
			"icon": "discord",
			"url": "https://discord.gg/fops"
		}
	],
	"feedback": {
		"suggestEdit": true,
		"raiseIssue": true,
		"feedback.thumbsRating": true
	},
	"search": {
		"prompt": "Search"
	},
	"tabs": [
    {
        "name": "Resource Packs",
        "url": "resourcepacks"
    },
    {
        "name": "Content",
        "url": "content"
    },
    {
        "name": "Blog",
        "url": "https://your-website.com/blog"
    }
],
	"primaryTab": {
    "name": "Home"
},
	"layout": "topnav",
	"sidebar": {
		"items": "container"
	},
	"api": {
		"auth": {
			"method": "bearer"
		}
	},
	"navigation": [
		{
			"group": "",
			"pages": [
				"introduction",
				"roadmap"
			]
		},
    {
      "group": "Essentials",
      "pages": [
        "essentials/markdown",
        "essentials/code",
        "essentials/images",
        "essentials/settings",
        "essentials/navigation",
        "essentials/reusable-snippets"
      ]
    }
	],
	"footerSocials": {
		"x": "https://twitter.com/UnboundClient",
		"github": "https://github.com/unbound-mod",
		"discord": "https://discord.gg/fops"
	}
}